{
  "manifest_version": 2,
  "name": "Cloud Save",
  "version": "1.4.1",
  "description": "Save to cloud.",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "*://*/",
    "tabs",
    "contextMenus",
    "unlimitedStorage",
    "notifications"
  ],
  "options_page": "settings.html",
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "64": "icon/64.png",
    "128": "icon/128.png",
    "512": "icon/512.png"
  }
}
